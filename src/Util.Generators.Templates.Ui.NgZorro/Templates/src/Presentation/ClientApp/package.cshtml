    "@@delon/auth": "16.4.3",
    "@@delon/cache": "16.4.3",
    "@@delon/chart": "16.4.3",
    "@@delon/form": "16.4.3",
    "@@delon/mock": "16.4.3",
    "@@delon/theme": "16.4.3",
    "@@delon/util": "16.4.3",
    "ngx-tinymce": "16.0.0",
    "screenfull": "6.0.2",
    "angular-oauth2-oidc": "15.0.1",
    "@@angular-architects/module-federation": "16.0.4",
    "lodash-es": "4.17.21",
    "date-fns": "2.30.0",
    "ng-event-bus": "5.2.0",
