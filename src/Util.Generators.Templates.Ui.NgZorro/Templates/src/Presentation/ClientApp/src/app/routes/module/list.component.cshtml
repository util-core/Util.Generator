@* Ui层项目 - 列表组件文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    Model.Output.FileNameNoExtension = service.GetClientListFileName();
    Model.Output.Extension = ".ts";
    Model.Output.RelativeRootPath = $"src/{service.GetClientComponentPath()}";
}
@if ( service.IsTreeEntity() ) {
    @Html.PartialAsync( "~/Templates/src/Presentation/ClientApp/src/app/routes/module/list/list.tree.component.Part.cshtml", Model )
}
else {
    @Html.PartialAsync( "~/Templates/src/Presentation/ClientApp/src/app/routes/module/list/list.component.Part.cshtml", Model )
}
