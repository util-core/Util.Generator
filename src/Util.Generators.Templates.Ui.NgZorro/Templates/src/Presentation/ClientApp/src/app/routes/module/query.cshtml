@* Ui层项目 - 查询参数文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    Model.Output.FileNameNoExtension = service.GetClientQueryFileName();
    Model.Output.Extension = ".ts";
    Model.Output.RelativeRootPath = $"src/{service.GetClientComponentPath()}/model";
}
@if ( service.IsTreeEntity() ) {
    @Html.PartialAsync( "~/Templates/src/Presentation/ClientApp/src/app/routes/module/query/query.tree.Part.cshtml", Model )
}
else {
    @Html.PartialAsync( "~/Templates/src/Presentation/ClientApp/src/app/routes/module/query/query.Part.cshtml", Model )
}