@* Ui层项目 - 编辑页文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    Model.Output.FileNameNoExtension = service.GetClientEditFileName();
    Model.Output.Extension = ".cshtml";
    Model.Output.RelativeRootPath = $"src/{service.GetClientComponentPath()}";
}
@if ( service.IsTreeEntity() ) {
    @Html.PartialAsync( "~/Templates/src/Presentation/ClientApp/src/app/routes/module/Pages/Edit/Edit.Tree.Part.cshtml", Model )
}
else {
    @Html.PartialAsync( "~/Templates/src/Presentation/ClientApp/src/app/routes/module/Pages/Edit/Edit.Part.cshtml", Model )
}
